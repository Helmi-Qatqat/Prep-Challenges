            "source": [
                "#####  [Migratory Birds](https://www.hackerrank.com/challenges/migratory-birds/problem?isFullScreen=true)\n\nSolution:"
            ],
            "outputs": []
        },
        {
            "language": "javascript",
            "source": [
                "function migratoryBirds(arr) {\n  let newArr = [];\n  let arrString = arr.join(\"\")\n  // let sortedArr = Array.from(arr).sort((a,b) => a - b)\n  let filteredArr = Array.from(new Set(arr));\n  for(let i of filteredArr) {\n    let ReString = new RegExp(i,\"ig\")\n    newArr.push(arrString.match(ReString))\n  }\n  let maxArrayLength = Math.max(...newArr.map(a => a.length));\n  return (newArr.filter(arr => arr.length === maxArrayLength).sort((a,b)=> a - b))[0][0]\n}"
            ],
