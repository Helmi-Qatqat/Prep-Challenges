                "function howManyGames(p, d, m, s) {\n  let gamesNum = 0;\n  let total = 0;\n  while (p <= s) {\n      gamesNum++;\n      s = s - p\n      if (p - d > m) { p = p - d }\n      else { p = m }\n  }\n  return gamesNum\n}"
